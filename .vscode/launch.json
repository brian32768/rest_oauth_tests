    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Flask Session",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "cwd": "${workspaceFolder}/flask_session_auth",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--cert=adhoc",
                "--host=10.10.10.149",
                "--port=5001",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": "true",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Flask OAUTH",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "cwd": "${workspaceFolder}/flask_dance_test",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--cert=adhoc",
                "--host=10.10.10.149",
                "--port=5001",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": "true",
            "console": "integratedTerminal"
        }

    ]
}
